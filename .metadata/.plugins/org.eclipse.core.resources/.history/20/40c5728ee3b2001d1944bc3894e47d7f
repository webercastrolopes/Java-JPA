package dominio;

import java.io.Serializable;
//import java.time.LocalDateTime;

import org.hibernate.type.descriptor.DateTimeUtils;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;

@Entity
public class Cotacao implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	private String numero;
	
	private String 			cod_for;
	private String 			cod_prd;
	//private LocalDateTime	   dt_orcado;
	private String 			marca;
	private String			pedido;
	private String 			quantidade;
	private cotaStatus		status;
	private Double 			vl_orcado;
	
	public Cotacao() {
		
	}

	

	public Cotacao(String numero, String cod_for, String cod_prd, String marca, String pedido, String quantidade,
			cotaStatus status, Double vl_orcado) {
		super();
		this.numero = numero;
		this.cod_for = cod_for;
		this.cod_prd = cod_prd;
		this.marca = marca;
		this.pedido = pedido;
		this.quantidade = quantidade;
		this.status = status;
		this.vl_orcado = vl_orcado;
	}



	public String getNumero() {
		return numero;
	}

	public void setNumero(String numero) {
		this.numero = numero;
	}

	public String getCod_for() {
		return cod_for;
	}

	public void setCod_for(String cod_for) {
		this.cod_for = cod_for;
	}

	public String getCod_prd() {
		return cod_prd;
	}

	public void setCod_prd(String cod_prd) {
		this.cod_prd = cod_prd;
	}

	

	public String getMarca() {
		return marca;
	}

	public void setMarca(String marca) {
		this.marca = marca;
	}

	public String getPedido() {
		return pedido;
	}

	public void setPedido(String pedido) {
		this.pedido = pedido;
	}

	public String getQuantidade() {
		return quantidade;
	}

	public void setQuantidade(String quantidade) {
		this.quantidade = quantidade;
	}

	public cotaStatus getStatus() {
		return status;
	}

	public void setStatus(cotaStatus status) {
		this.status = status;
	}

	public Double getVl_orcado() {
		return vl_orcado;
	}

	public void setVl_orcado(Double vl_orcado) {
		this.vl_orcado = vl_orcado;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	}

	@Override
	public String toString() {
		return "Cotacao [numero=" + numero + ", cod_for=" + cod_for + ", cod_prd=" + cod_prd + ", marca=" + marca
				+ ", pedido=" + pedido + ", quantidade=" + quantidade + ", status=" + status + ", vl_orcado="
				+ vl_orcado + "]";
	}

	
	
	
	
	
	
	
}
